{
	"name": "simple-react-boilerplate",
	"version": "1.0.0",
	"description": "Simple React Boilerplate",
	"author": "Felipe Zinser",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf dist/ && NODE_ENV=production webpack --config webpack.config.prod.js",
		"start": "webpack-dev-server --config webpack.config.dev.js",
		"server": "yarn build && cd dist && python -m SimpleHTTPServer 2222"
	},
	"reactSnap": {
		"source": "dist",
		"inlineCss": true
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"axios": "^0.18.0",
		"prop-types": "^15.6.2",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^5.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.2.3",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.5",
		"critical-plugin": "^1.0.6",
		"css-loader": "^2.1.0",
		"cssnano": "^4.1.8",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"image-webpack-loader": "^4.6.0",
